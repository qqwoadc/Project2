<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cskaoyan.dao.ReceiveTargetMapper">

    <!--模糊查找-->
    <select id="findRtByTeamName" parameterType="string" resultType="receivetarget">
        SELECT * from receivetarget WHERE teamName LIKE #{teamName}
    </select>

    <!--添加对象团体-->
    <insert id="addReceiveTarget" parameterType="receivetarget">
        insert into receivetarget values(#{teamCode},#{teamName},#{targetTypeName},#{principal},#{contactPhoneNUmber},#{registerTime})
    </insert>

    <!--修改团体信息-->
    <update id="updateRtByTeamCode" parameterType="receivetarget">
        UPDATE receivetarget SET teamName=#{teamName},principal=#{principal},contactPhoneNUmber=#{contactPhoneNUmber} WHERE teamCode=#{teamCode}
    </update>

    <!--删除团体信息-->
    <select id="deleteRtByTeamCode" parameterType="string" resultType="receivetarget">
        DELETE FROM receivetarget WHERE teamCode=#{teamCode}
    </select>

</mapper>